<div id="providers">
      <div class="secondMenuPage">
          <div class="collection clearfix">
            <div class="collectionTop">
              <div class="choice">
                <label for="payStatus">支付状态:</label>
                <select id="payStatus">
                 <option value="" selected>全部</option>
                 <option value="0">未支付</option>
                 <option value="1">已支付</option>
                </select>
              </div>
               <div class="choice">
                <label for="openingStatus">开通状态:</label>
                <select id="openingStatus">
                 <option value="" selected>全部</option>
                 <option value="0">未开通</option>
                 <option value="1">已开通</option>
                </select>
              </div>
            </div>           
            <div class="collectionBottom">
              <p>
                <label for="orderNumber">订单号</label>
                <input type="text" name="" id="orderNumber" autocomplete="off" placeholder="请输入14位订单号">
               </p>
               <p>
                <label for="applyName">应用名称</label>
                <input type="text" name="" id="applyName" autocomplete="off" placeholder="请输入应用名称">
              </p>
              <p>
                <label for="purchaser">采购方</label>
                <input type="text" name="" id="purchaser" autocomplete="off" placeholder="请输入采购方">
              </p>
              <p class="orderTime">
                <label for="providersDate">下单时间</label>
                <input class="datainp wicon" id="providersDate" type="text" placeholder="请选择日期" value="">
              </p>
              <span class="query">搜 索</span>
              <div style="clear: both;"></div>
            </div>
          </div>
      </div>
      <div class="tableLists">
        <table class="tableItem">
          <thead>
            <tr>
                <th>订单号</th>
                <th>商品名称</th>
                <th>销售规格</th>
                <th>支付金额</th>
                <th>采购方</th>
                <th>下单时间</th>
                <th>支付状态</th>
                <th class="on">开通状态</th>
            </tr>
          </thead>
          <tbody>
          
          </tbody>      
        </table>
        <ul class="page" maxshowpageitem="2" pagelistcount="5"  id="page" style=""></ul> 
      </div>
      
      <div class="Mask"></div>
  </div>
